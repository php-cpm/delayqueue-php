{
  "name": "php-cpm/delayqueue-php",
  "type": "library",
  "description": "PHP client for DelayQueue",
  "keywords": ["delayqueue","delayqueue-php"],
  "homepage": "https://github.com/ouqiang/delayqueue-php",
  "license": "MIT",
  "authors": [
    {
      "name": "ouqiang",
      "email": "qingqianludao@gmail.com",
      "homepage": "https://github.com/ouqiang"
    },{
      "name": "php-cpm"
    }
  ],
  "require": {
    "php": ">= 5.5",
    "ext-pcntl": "*",
    "monolog/monolog": "^1.0",
    "guzzlehttp/guzzle": "^6.3",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.5.*"
  },
  "bin": [
    "bin/delayqueue-php"
  ],
  "autoload": {
    "psr-4": {
      "DelayQueue\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
